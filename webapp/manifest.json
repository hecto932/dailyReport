{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "dailyReport",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"tiles": {
				"uri": "model/data.json",
				"type": "JSON"
			},
			"FARMS.farm": {
				"uri": "/destinations/Farms/farm.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "webapp/localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "dailyReport.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "dailyReport.i18n.i18n"
				}
			},
			"": {
				"dataSource": "FARMS.farm",
				"settings": {
					"user": "S0015228565",
					"password": "Hana123456"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "dailyReport.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "farm/{farmId}",
					"name": "farm",
					"target": "farm"
				},
				{
					"pattern": "farm/{farmId}/shed/{shedId}",
					"name": "sheds",
					"target": "sheds"
				}
			],
			"targets": {
				"home": {
					"viewName": "Farm",
					"viewType": "JS",
					"viewLevel": 1
				},
				"farm": {
					"viewName": "Shed",
					"viewType": "JS",
					"viewLevel": 2
				},
				"sheds": {
					"viewName": "Form",
					"viewType": "JS",
					"viewLevel": 3
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}